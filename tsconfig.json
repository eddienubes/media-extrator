{
  "compilerOptions": {
    "esModuleInterop": true,
    "strict": true,
    "declaration": true,
    "strictNullChecks": true,
    "target": "es2022",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "noImplicitAny": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "strictPropertyInitialization": false,
    "types": [
      "vitest/globals"
    ]
  }
}